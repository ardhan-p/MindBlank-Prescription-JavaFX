<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mindblank.doctor.boundaries.DoctorAddQRCodePopupUI">
   <children>
      <Button fx:id="closeBtn" layoutX="175.0" layoutY="349.0" mnemonicParsing="false" onAction="#onClose" text="Close" />
      <ImageView fx:id="qrCodeImageView" fitHeight="150.0" fitWidth="150.0" layoutX="125.0" layoutY="172.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="confirmedLabel" layoutX="34.0" layoutY="37.0" text="Confirmed!">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Label fx:id="successLabel" layoutX="34.0" layoutY="74.0" prefHeight="86.0" prefWidth="333.0" text="Email was successfully sent to example@email.com with the token exampleToken and with the QR code below." wrapText="true" />
   </children>
</AnchorPane>
